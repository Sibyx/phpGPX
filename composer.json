{
	"name": "sibyx/phpgpx",
	"type": "library",
	"version": "1.3.0",
	"description": "A simple PHP library for GPX manipulation",
	"minimum-stability": "stable",
	"keywords": [
		"gpx",
		"geospacial",
		"parser",
		"geo"
	],
	"homepage": "https://sibyx.github.io/phpGPX/",
	"readme": "README.md",
	"license": "MIT",
	"authors": [
		{
			"name": "Jakub Dubec",
			"email": "jakub.dubec@gmail.com",
			"homepage": "https://jakubdubec.me"
		}
	],
	"require": {
		"php": ">=8.1",
		"lib-libxml": "*",
		"ext-simplexml": "*",
		"ext-dom": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5.5",
		"friendsofphp/php-cs-fixer": "^v3.43.1"
	},
	"autoload": {
		"psr-4": { "phpGPX\\": "src/phpGPX" }
	},
	"autoload-dev": {
		"psr-4": { "phpGPX\\": "tests/phpGPX" }
	}
}
